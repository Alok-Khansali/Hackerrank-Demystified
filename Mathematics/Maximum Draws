import java.io.*;
import java.math.*;
import java.text.*;
import java.util.*;
import java.util.regex.*;
public class Solution {
    static int Draws(int n) 
    {
        return(n+1);
    }
    public static void main(String args[])
    {
        Scanner I=new Scanner(System.in);
        int t=I.nextInt();
        while(t-->0)
        {
            int n=I.nextInt();
            System.out.println(Draws(n));
        }
    }
}
