#include <bits/stdc++.h>
using namespace std;
int restaurant(int l, int b) {
    int x=__gcd(l,b);
    x=(l*b)/(x*x);
    return(x);
}
int main()
{
    int t;
    cin >> t;
    for (int t_itr = 0; t_itr < t; t_itr++){
        int l,b;
        cin>>l>>b;
        cout <<restaurant(l,b)<< "\n";
    }
    return 0;
}
