#include <bits/stdc++.h>
using namespace std;
long halloweenParty(int k) {
      return(pow(k/2,2)+(k/2)*(k%2)); 
}
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;
    cin >> t;
    for (int t_itr = 0; t_itr < t; t_itr++) {
        int k;
        cin >> k;
        long result = halloweenParty(k);
        cout << result << "\n";
    }
    return 0;
}


