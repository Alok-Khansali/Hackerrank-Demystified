#include <bits/stdc++.h>
using namespace std;
int findMedian(vector<int> v) {
    sort(v.begin(),v.end());
    return(v[v.size()/2]);
}
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int n;
    cin>>n;
    vector<int> arr(n);
    for (int i = 0; i < n; i++) 
      cin>>arr[i];
    cout <<findMedian(arr) << "\n";
    return 0;
}
