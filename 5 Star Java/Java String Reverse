import java.io.*;
import java.util.*;
public class Solution 
{
    public static void main (String args[])
     {   
        Scanner I=new Scanner(System.in);
        String A=I.next(),w="";
        for(int i=A.length()-1;i>=0;i--)
           w+=A.charAt(i);
        if(A.equals(w))
        System.out.print("Yes");
        else
        System.out.print("No");    
    }
}
