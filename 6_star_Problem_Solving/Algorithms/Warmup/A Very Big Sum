#include <bits/stdc++.h>
using namespace std;
long aVeryBigSum(vector<long> v) {
    long s=0;
       for (int i = 0; i <v.size(); i++)
       s+=v[i];
   return(s);
}
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int a;
    cin >> a;
    vector<long> v(a);
    for (int i = 0; i <a; i++)
      cin>>v[i];
    cout <<aVeryBigSum(v)<< "\n";
    return 0;
}
